{
  "name": "vue-ssr",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "node server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "preview": "cross-env NODE_ENV=production node server",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "ofetch": "^1.3.3",
    "sirv": "^2.0.4",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "@vitejs/plugin-vue": "^5.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vue-tsc": "^1.8.27"
  }
}
